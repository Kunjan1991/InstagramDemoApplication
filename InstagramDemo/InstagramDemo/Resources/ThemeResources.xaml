                        </Border>
                        <Border x:Name="ActionIconBorder" Background="Transparent" HorizontalAlignment="Right" Height="60" VerticalAlignment="Bottom" Width="60">
                            <Image x:Name="ActionIcon" Height="32"  Margin="10,18,-5,0" Source="{TemplateBinding ActionIcon}" Width="25"/>
                        </Border>
                        <TextBlock x:Name="MeasurementTextBlock" VerticalAlignment="Top" HorizontalAlignment="Left" FontWeight="{TemplateBinding FontWeight}" FontStyle="{TemplateBinding FontStyle}" FontStretch="{TemplateBinding FontStretch}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" IsHitTestVisible="False" Margin="8" Opacity="0" TextAlignment="Left" TextWrapping="{TemplateBinding TextWrapping}" Text="{TemplateBinding Text}"/>
                    </Grid>
                </ControlTemplate>
